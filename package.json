{
  "name": "solco",
  "version": "0.2.1",
  "description": "Solidity smart contracts utility",
  "main": "index.js",
  "scripts": {},
  "bin": {
    "solco": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderC/solco.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart contracts",
    "utility",
    "etherscan",
    "verify",
    "abi"
  ],
  "author": "AlexanderC",
  "epilog": "Made w/ ‚ù§ by AlexanderC <alexander.moldova@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ascii-table": "0.0.9",
    "chalk": "^2.3.0",
    "debug": "^3.1.0",
    "ethereumjs-abi": "^0.6.5",
    "fs-extra": "^5.0.0",
    "graphviz2svg": "^0.2.0",
    "open": "0.0.5",
    "ora": "^1.3.0",
    "pify": "^3.0.0",
    "pkg-dir": "^2.0.0",
    "resolve-from": "^4.0.0",
    "solc": "^0.4.19",
    "solgraph": "github:AlexanderC/solgraph",
    "solidity-parser": "^0.4.0",
    "solium": "^1.1.2",
    "treeify": "^1.0.1",
    "truffle-flattener": "^1.2.5",
    "yargs": "^10.1.1"
  },
  "solium": {
    "extends": "solium:all",
    "plugins": [
      "security"
    ],
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "indentation": [
        "error",
        4
      ],
      "arg-overflow": [
        "warning",
        3
      ]
    }
  }
}
